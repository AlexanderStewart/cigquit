\n      //   </div>\n      // );\n    } else {\n      if (validated) {\n        return <div>{props.children}</div>;\n      } else {\n        history.push(\"/login\");\n        localStorage.removeItem(\"token\");\n        return null;\n      }\n    }\n  }\n}\n\nexport default withRouter(Auth);\n","/Users/mac/Desktop/cig-quit/client/src/pages/home.js",["99","100"],"/Users/mac/Desktop/cig-quit/client/src/context/userProvider.js",[],"/Users/mac/Desktop/cig-quit/client/src/pages/signup.js",["101"],"/Users/mac/Desktop/cig-quit/client/src/constants.js",[],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/index.js",[],["102","103"],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/App.js",[],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/reportWebVitals.js",[],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/pages/signup.js",["104","105"],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/pages/home.js",["106"],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/pages/login.js",["107","108"],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/pages/setQuitStats.js",[],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/context/userProvider.js",[],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/pages/profile.js",[],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/context/Auth.js",["109","110"],"/Users/mac/Desktop/cig-quit-final/cigquit/client/src/constants.js",[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":19,"column":6,"nodeType":"117","endLine":19,"endColumn":8,"suggestions":"118"},{"ruleId":"119","severity":1,"message":"120","line":107,"column":42,"nodeType":"121","endLine":107,"endColumn":64},{"ruleId":"115","severity":1,"message":"122","line":21,"column":6,"nodeType":"117","endLine":21,"endColumn":8,"suggestions":"123"},{"ruleId":"124","severity":1,"message":"125","line":1,"column":8,"nodeType":"126","messageId":"127","endLine":1,"endColumn":13},{"rule